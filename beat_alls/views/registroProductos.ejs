<body>
<div class="contact_form">
    <div class="formulario">
    <form action="/registroProductos" method="post" enctype="multipart/form-data">
        <label for="Nombre">Nombre del producto</label>
        <input name="Nombre_producto" id="Nombre_producto" type="text">
        <br>
        <label for="Descripcion">Descripción</label>
        <input name="Descripcion" id="Descripcion" type="text">
        <br>
        <label for="Color">Color</label>
        <input name="Color" id="Color" type="text">
        <br>
        <label for="Talla">Talla</label>
        <input name="Talla" id="Talla" type="text">
        <br>
        <label for="Material">Material</label>
        <input name="Material" id="Material" type="text">
        <br>
        <label for="Marca">Marca</label>
        <input name="Marca" id="Marca" type="text">
        <br>
        <label for="Temporada">Temporada</label>
        <input name="Temporada" id="Temporada" type="text">
        <br>
        <label for="Precio">Precio</label>
        <input name="Precio" id="Precio" type="text">
        <br>
        <label for="Existencias">Existencias</label>
        <input name="Existencias" id="Existencias" type="text">
        <br>
        <label for="ID_Proveedor">ID Proveedor</label>
        <input name="ID_Proveedor" id="ID_Proveedor" type="text">
        <br>
        <label for="IMG">Imagen</label>
        <input name="IMG" id="IMG" type="file">
        <br><br>
        <button type="submit">Registrar producto</button>
    </form> 
    </div>
</div>
</body>
